				<skip.update-nightly-download-page>false</skip.update-nightly-download-page>

				<sts4-language-servers-version>nightly</sts4-language-servers-version>
			</properties>
		</profile>

		<profile>
			<id>milestone</id>
			<properties>
				<dist.type>milestone</dist.type>
